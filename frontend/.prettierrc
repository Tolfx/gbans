{
  "semi": true,
  "trailingComma": "none",
  "singleQuote": true,
  "tabWidth": 4,
  "endOfLine": "lf",
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^react$",
    "^react-.*$",
    "<THIRD_PARTY_MODULES>",
    "^[./]"
  ],
  "importOrderSeparation": false
}
